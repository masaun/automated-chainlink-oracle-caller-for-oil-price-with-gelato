{
  "name": "automated-chainlink-oracle-caller",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:local": "truffle test ./test/* --network local",
    "test:goerli": "truffle test ./test/* --network goerli",
    "test:rinkeby": "truffle test ./test/* --network rinkeby",
    "start-blank": "rm -rf client/src && cp -rf barebones/src client/",
    "client": "cd client && npm run start",
    "console:ropsten": "truffle console --network ropsten",
    "compile:ropsten": "truffle compile --reset --network ropsten",
    "migrate:ropsten": "truffle migrate --reset --network ropsten",
    "migrate:ropsten_quiknode": "truffle migrate --reset --network ropsten_quiknode",
    "console:kovan": "truffle console --network kovan",
    "compile:kovan": "truffle compile --reset --network kovan",
    "migrate:kovan": "truffle migrate --reset --network kovan",
    "console:rinkeby": "truffle console --network rinkeby",
    "compile:rinkeby": "truffle compile --reset --network rinkeby",
    "migrate:rinkeby": "truffle migrate --reset --network rinkeby",
    "compile:goerli": "truffle compile --reset --network goerli",
    "migrate:goerli": "truffle migrate --reset --network goerli",
    "migrate:local": "truffle migrate --reset --network local"
  },
  "author": "masaun",
  "license": "MIT",
  "dependencies": {
    "@gelatonetwork/core": "0.5.3",
    "@gelatonetwork/gelato-user-proxy": "1.0.0",
    "@openzeppelin/contracts": "^3.1.0",
    "@truffle/hdwallet-provider": "^1.0.29",
    "dotenv": "^8.2.0",
    "eth-block-tracker": "^4.4.3"
  },
  "devDependencies": {}
}
